{
	"name": "zappit",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "nodemon --config ./nodemon.json ./server/index.ts --dev true",
		"build": "next build",
		"start": "npm-run-all start:*",
		"start:install": "npm i",
		"start:clean": "rm -rf ./build/*",
		"start:build-server": "tsc -p ./server/tsconfig.json",
		"start:build-client": "next build",
		"start:move-static": "cp -r ./locales ./build/ && cp -r ./public ./build/",
		"start:run-server": "node ./build/server/index.js --dev false",
		"lint": "next lint && npx eslint . --ext .js,.jsx,.ts,.tsx",
		"test": "jest --coverage"
	},
	"dependencies": {
		"@types/socket.io": "^3.0.2",
		"axios": "^1.1.3",
		"bcrypt": "^5.0.1",
		"body-parser": "^1.20.0",
		"bootstrap": "^5.2.2",
		"compression": "^1.7.4",
		"connect-mongo": "^4.6.0",
		"cookie-parser": "^1.4.6",
		"dotenv": "^16.0.3",
		"express-rate-limit": "^6.6.0",
		"express-session": "^1.17.3",
		"framer-motion": "^7.6.1",
		"helmet": "^6.0.0",
		"locale": "^0.1.0",
		"mongoose": "^6.6.5",
		"ms": "^2.1.3",
		"next": "12.3.1",
		"nodemailer": "^6.8.0",
		"nodemon": "^2.0.20",
		"npm-run-all": "^4.1.5",
		"passport": "^0.6.0",
		"passport-local-mongoose": "^7.1.2",
		"react": "18.2.0",
		"react-bootstrap": "^2.5.0",
		"react-dom": "18.2.0",
		"react-tsparticles": "^2.3.3",
		"redis": "^4.3.1",
		"sass": "^1.55.0",
		"serve-favicon": "^2.5.0",
		"speakeasy": "^2.0.0",
		"tsparticles": "^2.3.4",
		"uuid": "^9.0.0",
		"validator": "^13.7.0"
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.0",
		"@types/compression": "^1.7.2",
