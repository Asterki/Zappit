		"@types/react": "18.0.21",
		"@types/react-dom": "18.0.6",
		"@types/serve-favicon": "^2.5.3",
		"@types/speakeasy": "^2.0.7",
		"@types/uuid": "^8.3.4",
		"@types/validator": "^13.7.7",
		"@typescript-eslint/eslint-plugin": "^5.39.0",
		"eslint": "8.24.0",
